{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/api/contact/route.js",
    "app/lib/mongodb.jsx",
    "app/Places/page.jsx",
    "components/InformationCard.jsx",
    "components/Chatbot.jsx",
    "components/ContactForm.jsx",
    "components/GoogleMapView.jsx",
    "components/Header.jsx",
    "components/Hero.jsx",
    "components/Marker.jsx",
    "components/LandingBottom.jsx",
    "context/UserLocationContext.jsx",
    "context/PlaceListContext.jsx",
    "app/api/google-place-api/route.js",
    "components/PlaceList.jsx",
    "app/models/contact.js]"
  ],
  "exclude": [
    "node_modules"
  ]
}
